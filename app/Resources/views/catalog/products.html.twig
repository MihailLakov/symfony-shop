{% extends 'base.html.twig' %}
{% block body %}
<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <h2>Products Catalog</h2>
        </div>
        {% for product in products %}
        <div class="col-xs-12 col-sm-6 col-md-4">
           {{ render(controller('ShopBundle:Product:singleProductTemplate', {'id': product.id})) }}
        </div>
        {% endfor %}
    </div>
</div>
{% endblock %}
